{
    "name": "langium-domainmodel-dsl",
    "displayName": "Domain Model DSL",
    "version": "1.2.0",
    "description": "Example language built with Langium",
    "homepage": "https://langium.org",
    "engines": {
        "vscode": "^1.67.0"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "domain-model",
                "aliases": [
                    "DomainModel",
                    "domain-model"
                ],
                "extensions": [
                    ".dmodel"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "domain-model",
                "scopeName": "source.domain-model",
                "path": "./syntaxes/domain-model.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:domain-model"
    ],
    "files": [
        "bin",
        "out",
        "src",
        "syntaxes",
        "language-configuration.json"
    ],
    "bin": {
        "domainmodel-cli": "./bin/cli"
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run build && npm run lint",
        "build": "tsc",
        "watch": "tsc -watch",
        "lint": "eslint src test --ext ts",
        "benchmark": "node out/benchmark/run.js",
        "langium:generate": "langium generate",
        "langium:watch": "langium generate --watch",
        "publish:next": "npm --no-git-tag-version version \"$(semver $npm_package_version -i minor)-next.$(git rev-parse --short HEAD)\" && npm publish --tag next",
        "publish:latest": "npm publish --tag latest --access public"
    },
    "dependencies": {
        "chalk": "~4.1.2",
        "commander": "~10.0.0",
        "langium": "~1.2.0",
        "lodash": "~4.17.21",
        "vscode-languageclient": "~8.0.2",
        "vscode-languageserver": "~8.0.2"
    },
    "devDependencies": {
        "langium-cli": "~1.2.0"
    },
    "volta": {
        "node": "16.19.0",
        "npm": "8.19.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/langium/langium",
        "directory": "examples/domainmodel"
    },
    "bugs": "https://github.com/langium/langium/issues",
    "author": {
        "name": "TypeFox",
        "url": "https://www.typefox.io"
    }
}
