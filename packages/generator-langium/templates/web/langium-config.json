{
    "projectName": "<%= LanguageName %>",
    "languages": [{
        "id": "<%= language-id %>",
        "grammar": "src/language/<%= language-id %>.langium",
        "fileExtensions": <%= file-extension %>,
        "textMate": {
            "out": "syntaxes/<%= language-id %>.tmLanguage.json"
        },
        "monarch": {
            "out": "src/syntaxes/<%= language-id %>.monarch.ts"
        }
    }],
    "out": "src/language/generated"
}
