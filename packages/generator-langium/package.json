{
  "name": "generator-langium",
  "version": "1.2.0",
  "description": "Yeoman generator for Langium - the language engineering tool",
  "homepage": "https://langium.org",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "yeoman-generator",
    "language",
    "dsl",
    "lsp",
    "vscode"
  ],
  "license": "MIT",
  "files": [
    "app",
    "langium-template",
    "src"
  ],
  "main": "app/index.js",
  "types": "app/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf app",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src test --ext .ts",
    "run": "yo langium",
    "test": "vitest run",
    "debug": "npx --node-arg=--inspect yo langium",
    "publish:next": "npm --no-git-tag-version version \"$(semver $npm_package_version -i minor)-next.$(git rev-parse --short HEAD)\" && npm publish --tag next",
    "publish:latest": "npm publish --tag latest"
  },
  "dependencies": {
    "chalk": "~4.1.2",
    "lodash": "~4.17.21",
    "which": "~2.0.2",
    "yeoman-generator": "~5.7.0"
  },
  "devDependencies": {
    "@types/lodash": "~4.14.191",
    "@types/which": "~2.0.1",
    "@types/yeoman-generator": "~5.2.11",
    "@types/yeoman-test": "~4.0.3",
    "yeoman-test": "~7.3.0"
  },
  "volta": {
    "node": "16.19.0",
    "npm": "8.19.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/langium/langium",
    "directory": "packages/generator-langium"
  },
  "bugs": "https://github.com/langium/langium/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
