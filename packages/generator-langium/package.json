{
  "name": "generator-langium",
  "version": "0.2.1",
  "description": "Yeoman generator for Langium - the language engineering tool",
  "engines": {
    "node": ">=12.10.0"
  },
  "keywords": [
    "yeoman-generator",
    "language",
    "dsl",
    "lsp",
    "vscode"
  ],
  "license": "MIT",
  "files": [
    "app",
    "langium-template",
    "src"
  ],
  "main": "app/index.js",
  "types": "app/index.d.ts",
  "scripts": {
    "clean": "rimraf app",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "run": "yo langium",
    "debug": "npx --node-arg=--inspect yo langium",
    "publish:next": "npm --no-git-tag-version version \"$(semver $npm_package_version -i minor)-next.$(git rev-parse --short HEAD)\" && npm publish --tag next",
    "publish:latest": "npm publish --tag latest"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "lodash": "^4.17.21",
    "yeoman-generator": "^5.5.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.12.6",
    "@types/yeoman-generator": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.28.0",
    "eslint-plugin-header": "^3.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/langium/langium",
    "directory": "packages/generator-langium"
  },
  "bugs": "https://github.com/langium/langium/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
