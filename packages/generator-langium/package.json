{
  "name": "generator-langium",
  "version": "3.4.0",
  "description": "Yeoman generator for Langium - the language engineering tool",
  "homepage": "https://langium.org",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "keywords": [
    "yeoman-generator",
    "language",
    "dsl",
    "lsp",
    "vscode"
  ],
  "license": "MIT",
  "files": [
    "app",
    "templates",
    "src"
  ],
  "type": "module",
  "exports": "./app/index.js",
  "scripts": {
    "clean": "shx rm -rf app",
    "build": "tsc",
    "watch": "tsc --watch",
    "run": "yo langium",
    "test": "vitest run",
    "debug": "npx --node-arg=--inspect yo langium",
    "publish:next": "npm --no-git-tag-version version \"$(semver $npm_package_version -i minor)-next.$(git rev-parse --short HEAD)\" && npm publish --tag next",
    "publish:latest": "npm publish --tag latest --access public"
  },
  "dependencies": {
    "chalk": "~5.4.1",
    "lodash": "~4.17.21",
    "which": "~5.0.0",
    "yeoman-generator": "~7.1.1"
  },
  "devDependencies": {
    "@types/lodash": "~4.17.16",
    "@types/which": "~3.0.4",
    "@yeoman/types": "~1.6.0",
    "yeoman-environment": "~4.1.3",
    "yeoman-test": "~8.2.0"
  },
  "volta": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-langium/langium",
    "directory": "packages/generator-langium"
  },
  "bugs": "https://github.com/eclipse-langium/langium/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
